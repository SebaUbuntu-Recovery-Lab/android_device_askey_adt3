<thermalconfigs>
    <cooling_device name="cpul_cdev">
        <trips temp="80" state="0xf"/>
        <trips temp="95" state="0x7"/>
        <trips temp="100" state="0x3"/>
        <trips temp="105" state="0x1"/>
    </cooling_device>
    <cooling_device name="cpub_cdev">
        <trips temp="80" state="0xf"/>
        <trips temp="95" state="0x3"/>
        <trips temp="100" state="0x1"/>
        <trips temp="105" state="0x0"/>
    </cooling_device>
    <cooling_device name="dsu_cdev">
        <trips temp="70" state="1"/>
    </cooling_device>
    <cooling_device name="bat_cdev">
        <trips temp="60" state="2"/>
        <trips temp="70" state="1"/>
    </cooling_device>
</thermalconfigs>
