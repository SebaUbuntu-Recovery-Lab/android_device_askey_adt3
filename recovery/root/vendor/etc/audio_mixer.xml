<mixer>
    <!-- playback -->
    <path name="AUDIO_DEVICE_OUT_WIRED_HEADPHONE">
        <!-- reset -->
        <ctl name="HPOUT Switch" value="0" />
        <ctl name="SPK Switch" value="0" />
        <ctl name="LINEOUTL Switch" value="0" />
        <ctl name="LINEOUTR Switch" value="0" />

        <!-- set -->
        <ctl name="LINEOUT Gain" value="31" />
        <ctl name="LINEOUTL Switch" value="1" />
        <ctl name="LINEOUTR Switch" value="1" />
        <ctl name="SPK Switch" value="0" />
        <ctl name="HPOUT Switch" value="1" />
    </path>
</mixer>
